contract sysparams_RoleUnassign {
    data {
        RId int
    }
    
    conditions {
        $raId = DBFind("roles_assign").Where("id=$", $RId).One("id")
        if $raId == nil {
            warning "Role not found for the specified member"
        }
    }
    
    action {
        DBUpdate("roles_assign", Int($raId), "delete,timestamp date_end", 1, $block_time)
    }
}