contract VotingSysparamInit {
    data {
        Name string
        Value string
        Lang string "optional"
    }

    conditions {
        if GetContractByName($Name) == 0 {
            warning LangRes("@1system_parameter_not found", $Lang)
        }
    }
    
    action {
        var params map
        params["Value"] = $Value
        CallContract($Name, params)
        // InvestorRefresh()    
    }
}