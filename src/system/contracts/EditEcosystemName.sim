contract EditEcosystemName {
    data {
        EcosystemID int
        NewName string
    }
    conditions {
        var rows array
        rows = DBFind("@1ecosystems").Where({id: $EcosystemID})
        if !Len(rows) {
            error Sprintf(LangRes("@1template_eco_not_exist", "en"), $EcosystemID)
        }
    }
    action {
        EditEcosysName($EcosystemID, $NewName)
    }
}